{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:53582",
      "sslPort": 44325
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/messages",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionString": "mongodb://root:example@mongo:27017/admin"
      }
    },
    "ErrorMessagesAPI": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/messages",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ConnectionString": "mongodb://root:example@mongo:27017/admin"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/messages",
      "environmentVariables": {
        "ConnectionString": "mongodb://root:example@mongo:27017/admin"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}